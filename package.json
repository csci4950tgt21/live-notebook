{
	"name": "live-notebook",
	"displayName": "live-notebook",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:plaintext"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "live-notebook.helloWorld",
				"title": "Hello World"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Live Notebook",
			"properties": {
				"live-notebook.virustotal_api_key": {
					"type": "string",
					"order": 3,
					"scope": "application",
					"description": "API Key for VirusTotal"
				},
				"live-notebook.regexes": {
					"type": "array",
					"order": 0,
					"default": [
						{
							"type": "URL",
							"regex": "(?<=\\s|^)(https?:\/\/)?(www\\.)?[a-z0-9]+([\\-\\.]{1}[a-z0-9]+)*\\.[a-z]{2,5}(:[0-9]{1,5})?(\/.*)?(?=\\s|$)"
						},
						{
							"type": "IP",
							"regex": "(?<=\\s|^)(\\d{1,3}\\.){3}\\d{1,3}(?=\\s|$)|(?<=\\s|^)(\\w{1,4}\\:){7}\\w{1,4}(?=\\s|$)|(?<=\\s|^)\\w{1,4}\\:(\\w{0,4}\\:){5}\\w{1,4}(?=\\s|$)"
						},
						{
							"type": "EMAIL",
							"regex": "(?<=\\s|^)[a-z0-9\\-\\.\\+]+@[a-z0-9\\-]+\\.[a-z0-9]+(?=\\s|$)"
						}
					],
					"scope": "application",
					"description": "Strings of Interest (Regular Expressions)"
				},
				"live-notebook.apis": {
					"type": "array",
					"order": 1,
					"default": [
						{
							"name": "Google Safe Browsing",
							"url": "https://safebrowsing.googleapis.com/v4/threatMatches:find",
							"method": "POST",
							"type": [
								"URL"
							],
							"query": {
								"key": "{API_KEY_HERE}"
							},
							"request": {
								"threatInfo": {
									"threatTypes": [
										"UNWANTED_SOFTWARE",
										"MALWARE",
										"THREAT_TYPE_UNSPECIFIED",
										"SOCIAL_ENGINEERING",
										"POTENTIALLY_HARMFUL_APPLICATION"
									],
									"platformTypes": [
										"ANY_PLATFORM"
									],
									"threatEntryTypes": [
										"URL"
									],
									"threatEntries": [
										{
											"url": "{live-notebook.stringOfInterest}"
										}
									]
								}
							},
							"mapping": {
								"link_self": "matches.0.threat.url",
								"type": "matches.0.threatEntryType"
							}
						},
						{
							"name": "VirusTotal IP",
							"url": "https://www.virustotal.com/api/v3/ip_addresses/{live-notebook.stringOfInterest}",
							"method": "GET",
							"type": [
								"IP"
							]
						},
						{
							"name": "VirusTotal URL",
							"method": "VirusTotal",
							"type": [
								"URL"
							]
						}
					],
					"scope": "application",
					"description": "APIs for Types of Strings"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.21.0",
		"form-data": "^3.0.0",
		"promise": "^8.1.0"
	}
}